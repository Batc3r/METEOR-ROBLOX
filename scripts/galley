    {Name = "Range", Min = 5, Max = 100, Default = 30, Callback = function(S)
            co = S
        end}
)
J:AddSlider(
    {Name = "Aim fov", Min = 0, Max = 360, Default = 30, Callback = function(S)
            cp = S
        end}
)
game:GetService("RunService").RenderStepped:Connect(
    function(aK)
        cv(aK)
    end
