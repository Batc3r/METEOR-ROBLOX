local dh = {Ambient = nil, OutdoorAmbient = nil, FogColor = nil, FogStart = nil, FogEnd = nil, PartColors = {}}
local di, dj, dk = Color3.new(225, 160, 255), Color3.fromRGB(225, 160, 255), Vector3.new(2, 1, 3)
local dl = false
local function dm(af)
    local dn = game:GetService("Lighting")
    if af then
        dh.Ambient = dn.Ambient
        dh.OutdoorAmbient = dn.OutdoorAmbient
        dh.FogColor = dn.FogColor
        dh.FogStart = dn.FogStart
        dh.FogEnd = dn.FogEnd
        for X, ay in ipairs(workspace:GetDescendants()) do
            if ay:IsA("BasePart") then
                dh.PartColors[ay] = ay.Color
            end
        end
        dn.Ambient = di
        dn.OutdoorAmbient = di
        dn.FogColor = di
        dn.FogStart = dh.FogStart or 0
        dn.FogEnd = dh.FogEnd or 100
        for X, ay in ipairs(workspace:GetDescendants()) do
            if ay:IsA("BasePart") then
                ay.Color = di
            end
        end
    else
        dn.Ambient = dh.Ambient
        dn.OutdoorAmbient = dh.OutdoorAmbient
        dn.FogColor = dh.FogColor
        dn.FogStart = dh.FogStart
        dn.FogEnd = dh.FogEnd
        for X, ay in ipairs(workspace:GetDescendants()) do
            if ay:IsA("BasePart") and dh.PartColors[ay] then
                ay.Color = dh.PartColors[ay]
            end
        end
    end
end
N:AddToggle(
    {Name = "Ambient", Callback = function(af)
            dm(af)
        end}
)
local dp = {}
N:AddToggle(
    {Name = "Low gfx", Callback = function(dg)
            local workspace = game:GetService("Workspace")
            for X, ay in ipairs(workspace:GetDescendants()) do
                if ay:IsA("BasePart") then
                    if dg then
                        dp[ay] = ay.Material
                        ay.Material = Enum.Material.Ice
                    else
                        ay.Material = dp[ay] or ay.Material
                        dp[ay] = nil
                    end
                end
            end
        end}
)
local function dq()
    local dr = game:GetService("Workspace").Camera:FindFirstChild("Viewmodel")
    if dr then
        local ds = dr:FindFirstChild("WoodenSword")
        if ds then
            local dt = ds:FindFirstChild("MainPart")
            if dt then
                return dt:FindFirstChild("Mesh")
            end
        end
    end
    return nil
end
N:AddToggle(
    {Name = "Custom sword", Callback = function(af)
            dl = af
            if not dl then
                local du = dq()
                if du then
                    du.Offset = Vector3.new(0, 0, 0)
                end
            end
        end}
)
game:GetService("RunService").Heartbeat:Connect(
    function()
        if dl then
            local du = dq()
            if du then
                du.Offset = dk
            end
        end
    end
)
local function dv()
    if dl then
        local du = dq()
        if du then
            du.Offset = dk
        end
    end
end
