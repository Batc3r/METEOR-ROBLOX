local cm, cn, co, cp, ar = false, 1, 30, 30, "Head"
local function cq(cr, cs, ct, cu)
    return cr:Lerp(cs, ct * cu)
end
local function cv(aK)
    if cm then
        local cw = nil
        local W = math.huge
        local aw = game:GetService("Players").LocalPlayer
        local cx = aw.Character
        if cx then
            local cy = cx:FindFirstChild(ar).Position
            local cz = cx:FindFirstChild(ar).CFrame.lookVector
            for X, Y in ipairs(game:GetService("Players"):GetPlayers()) do
                if Y ~= aw then
                    local cA = Y.Character
                    if cA then
                        local cB = cA:FindFirstChild(ar) and cA:FindFirstChild(ar).Position
                        if cB then
                            local _ = (cy - cB).magnitude
                            if _ < W and _ <= co then
                                local cC = (cB - cy).unit
                                local b4 = math.acos(cz:Dot(cC)) * 180 / math.pi
                                if b4 <= cp then
                                    W = _
                                    cw = Y
                                end
                            end
                        end
                    end
                end
            end
        end
        if cw then
            local cD = game:GetService("Workspace").CurrentCamera.CFrame
            local cE = cw.Character:FindFirstChild(ar) and cw.Character:FindFirstChild(ar).Position
            if cE then
                local cF = CFrame.new(cD.Position, cE)
                local cG = cq(cD, cF, aK, cn)
                game:GetService("Workspace").CurrentCamera.CFrame = cG
            end
        end
    end
end
J:AddToggle(
    {Name = "Aim Assist", Keybind = 1, Callback = function(af)
            cm = af
        end}
)
J:AddDropdown(
    {
        Name = "Target Part",
