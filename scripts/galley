local gui
local function cL()
    if gui then
        gui:Destroy()
    end
    gui = Instance.new("ScreenGui")
    gui.Name = "PlayerInfoGUI"
    gui.ResetOnSpawn = false
    gui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    local cM = Instance.new("Frame", gui)
    cM.Name = "Frame"
    cM.Size = UDim2.new(0.1, 0, 0.08, 0)
    cM.Position = UDim2.new(cJ, 0, cK, 0)
    cM.BackgroundColor3 = Color3.new(0, 0, 0)
    cM.BackgroundTransparency = 0.5
    cM.BorderSizePixel = 2
    cM.BorderColor3 = Color3.fromRGB(148, 0, 211)
    cM.Active = true
    local cN = Instance.new("UICorner", cM)
    cN.CornerRadius = UDim.new(0, cI)
    local cO = Instance.new("TextLabel", cM)
    cO.Text = "..."
    cO.TextColor3 = Color3.new(1, 1, 1)
    cO.Size = UDim2.new(1, 0, 0.3, 0)
    cO.Position = UDim2.new(0, 0, 0, 0)
    cO.BackgroundTransparency = 1
    cO.Font = Enum.Font.SourceSansBold
    cO.TextSize = 20
    cO.TextWrapped = true
    cO.TextXAlignment = Enum.TextXAlignment.Center
    local cP = Instance.new("TextLabel", cM)
    cP.Text = ""
    cP.TextColor3 = Color3.new(1, 1, 1)
    cP.Size = UDim2.new(1, 0, 0.3, 0)
    cP.Position = UDim2.new(0, 0, 0.3, 0)
    cP.BackgroundTransparency = 1
    cP.Font = Enum.Font.SourceSansBold
    cP.TextSize = 20
    cP.TextWrapped = true
    cP.TextXAlignment = Enum.TextXAlignment.Center
    local cQ = Instance.new("Frame", cM)
    cQ.Size = UDim2.new(0.6, 0, 0.2, 0)
    cQ.Position = UDim2.new(0.2, 0, 0.7, 0)
    cQ.BackgroundColor3 = Color3.new(0, 0, 0)
    cQ.BorderSizePixel = 2
    cQ.BorderColor3 = Color3.new(0, 0, 0)
    local cR = Instance.new("Frame", cM)
    cR.Size = UDim2.new(0.6, 0, 0.2, 0)
    cR.Position = UDim2.new(0.2, 0, 0.7, 0)
    cR.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    cR.BorderSizePixel = 0
    local cS = Instance.new("TextLabel", cM)
    cS.Text = ""
    cS.TextColor3 = Color3.new(1, 0, 0)
    cS.Size = UDim2.new(0.6, 0, 0.3, 0)
    cS.Position = UDim2.new(0.615, 0, 0.7, 0)
    cS.BackgroundTransparency = 1
    cS.Font = Enum.Font.SourceSansBold
    cS.TextSize = 16
    cS.TextWrapped = true
    cS.TextXAlignment = Enum.TextXAlignment.Center
    local cT = Instance.new("Frame", cM)
    cT.Size = UDim2.new(1, 0, 0, 2)
    cT.Position = UDim2.new(0, 0, 0.3, 0)
    cT.BorderSizePixel = 0
    cT.BackgroundColor3 = Color3.fromRGB(148, 0, 211)
    cT.BackgroundTransparency = 0.5
    local cU = Instance.new("Frame", cM)
    cU.Size = UDim2.new(1, 4, 1, 4)
    cU.Position = UDim2.new(-0.01, 0, -0.01, 0)
    cU.BackgroundColor3 = Color3.fromRGB(100, 0, 150)
    cU.BorderSizePixel = 0
    cU.ZIndex = -1
    local cV = Instance.new("TextLabel", cM)
    cV.TextColor3 = Color3.new(1, 1, 1)
    cV.Size = UDim2.new(1, 0, 0.1, 0)
    cV.Position = UDim2.new(0.05, 0, cS.Position.Y.Scale, cS.Position.Y.Offset - -8)
    cV.BackgroundTransparency = 1
    cV.Font = cS.Font
    cV.TextSize = cS.TextSize
    cV.TextWrapped = true
    cV.TextXAlignment = Enum.TextXAlignment.Left
    local function cW()
        local aj, ak = nil, math.huge
        local cX = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        local cY = false
        for X, Y in ipairs(game.Players:GetPlayers()) do
            local Z = Y.Character
            if Y ~= game.Players.LocalPlayer and Z and Z:FindFirstChild("HumanoidRootPart") then
                local _ = (Z.HumanoidRootPart.Position - cX).magnitude
                if _ < cH then
                    cY = true
                    if _ < ak then
                        aj, ak = Y, _
                    end
                end
            end
        end
        if cY then
            cO.Text = aj.Name
            local cZ = math.floor(aj.Character.Humanoid.Health)
            local c_ = tostring(cZ)
            local d0 = aj.Character.Humanoid.MaxHealth
            local d1 = cZ / d0
            local d2 = UDim2.new(0.6 * d1, 0, 0.2, 0)
            local b0 = TweenInfo.new(0.18, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
            local d3 = d:Create(cR, b0, {Size = d2})
            d3:Play()
            local c0 = Color3.fromRGB(255 * (1 - d1), 255 * d1, 0)
            cR.BackgroundColor3 = c0
            local d4 = cP.Text
            local d5 = 1
            local d6 = math.max(#d4, #c_)
            local d7 = 0.08
            local d8
            d8 =
                game:GetService("RunService").Heartbeat:Connect(
                function()
                    if d5 <= d6 then
                        cP.Text = c_:sub(1, d5)
                        d5 = d5 + 1
                    else
                        d8:Disconnect()
                    end
                end
            )
            cP.Visible = true
            cQ.Visible = true
            cR.Visible = true
            local d9 = math.floor(game.Players.LocalPlayer.Character.Humanoid.Health)
            if d9 > cZ then
                cS.Text = "winning"
                cS.TextColor3 = Color3.new(0, 1, 0)
            elseif d9 < cZ then
                cS.Text = "losing"
                cS.TextColor3 = Color3.new(1, 0, 0)
            else
                cS.Text = "even"
                cS.TextColor3 = Color3.fromRGB(44, 25, 239)
            end
            cS.Font = Enum.Font.FredokaOne
            local da = string.format("%.1f", ak)
            if cV.Text ~= da then
                local db = cV.Text
                local dc = 1
                local dd = math.max(#db, #da)
                local de = 0.03
                local df
                df =
                    game:GetService("RunService").Heartbeat:Connect(
                    function()
                        if dc <= dd then
                            cV.Text = da:sub(1, dc)
                            dc = dc + 1
                        else
                            df:Disconnect()
                        end
                    end
                )
            end
            cV.Visible = true
            cS.Visible = true
            cM.Visible = true
        else
            cO.Text = ""
            cP.Visible = false
            cQ.Visible = false
            cR.Visible = false
            cS.Visible = false
            cV.Visible = false
            cM.Visible = false
        end
    end
    game:GetService("RunService").Heartbeat:Connect(cW)
end
