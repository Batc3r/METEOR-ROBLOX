H:AddToggle(
    {
        Name = "Trigger bot",
        Keybind = 1,
        Callback = function(af)
            triggerbotActive = af
            local ca
            local cb = 0
            local minInterval = 1 / cpslimit
            local function cc(cd)
                for X, Y in pairs(game.Players:GetPlayers()) do
                    if Y.Character and cd and cd:IsDescendantOf(Y.Character) then
                        return true
                    end
                end
                return false
            end
            if af then
                ca =
                    game:GetService("RunService").Heartbeat:Connect(
                    function()
                        if cc(game.Players.LocalPlayer:GetMouse().Target) then
                            local ce = tick()
                            if ce - cb >= minInterval then
                                cb = ce
                                mouse1press()
                                task.wait()
                                mouse1release()
                            end
                        end
                    end
                )
                getgenv().disable = function()
                    if ca then
                        warn("Disconnecting:", ca)
                        ca:Disconnect()
                        ca = nil
                    end
                    getgenv().disable = nil
                end
            else
                if getgenv().disable then
                    getgenv().disable()
                end
            end
        end
    }
)
