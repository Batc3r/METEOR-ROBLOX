    {Name = "CPS limit", Min = 1, Max = 50, Def = cpslimit, Callback = function(bS)
            cpslimit = bS
            minInterval = 1 / cpslimit
        end}
)
local cf, cg, ch, ci, cj = "None", 0, 0, 0, 0
local function ck(S)
    settings():GetService("NetworkSettings").IncomingReplicationLag = S
end
I:AddDropdown(
    {
        Name = "Method",
        List = {"ReplicationLag", "Axis", "None"},
        Default = cf,
        Callback = function(au)
            cf = au
            if cf == "ReplicationLag" then
                getgenv().Axis = false
                ck(cg)
            elseif cf == "Axis" then
                getgenv().Axis = true
                getgenv().Z = cj
                getgenv().Y = ci
                getgenv().X = ch
                game:GetService("RunService").Heartbeat:Connect(
                    function()
                        if getgenv().Axis then
                            local cl = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
                            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity =
                                Vector3.new(getgenv().X, getgenv().Y, getgenv().Z)
                            game:GetService("RunService").RenderStepped:Wait()
                            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = cl
                        end
                    end
                )
                ck(0)
            end
        end
    }
)
I:AddSlider(
    {Name = "Client Lag", Min = 0, Max = 10, Precise = 2, Def = cg, Callback = function(bS)
            cg = bS
            if cf == "ReplicationLag" then
                ck(cg)
            end
        end}
)
