local ag
H:AddToggle(
    {
        Name = "Double click",
        Callback = function(af)
            c7 = af
            if af then
                ag =
                    c5.InputBegan:Connect(
                    function(c8, c9)
                        if not c9 and c8.UserInputType == Enum.UserInputType.MouseButton1 and not c6 then
                            task.wait(0.1)
                            c6 = true
                            mouse1click()
                            c6 = false
                        end
                    end
                )
            elseif ag then
                ag:Disconnect()
            end
        end
    }
