            dk = Vector3.new(dk.X, dk.Y, S)
            dv()
        end}
)
N:AddSlider(
    {Name = "Fog Start", Min = 0, Max = 1000, Value = dh.FogStart or 0, Callback = function(S)
            dh.FogStart = S
            if dl then
                game:GetService("Lighting").FogStart = S
            end
        end}
)
N:AddSlider(
    {Name = "Fog End", Min = 0, Max = 1000, Value = dh.FogEnd or 100, Callback = function(S)
            dh.FogEnd = S
            if dl then
                game:GetService("Lighting").FogEnd = S
            end
        end}
)
N:AddColorpicker(
    {Name = "Atmo color", Value = di, Callback = function(dw, dx)
            di = dw
            if af then
                local dn = game:GetService("Lighting")
                dn.Ambient = di
                dn.OutdoorAmbient = di
                dn.FogColor = di
                for X, ay in ipairs(workspace:GetDescendants()) do
                    if ay:IsA("BasePart") then
                        ay.Color = di
                    end
                end
            end
        end}
)
local dy, dz = 1, false
getgenv().res = dy
N:AddToggle(
    {Name = "ress", Callback = function(dg)
            dz = dg
            if dg then
                while dz do
                    game:GetService("RunService").RenderStepped:Wait()
                    j.CFrame = j.CFrame * CFrame.new(0, 0, 0, 1, 0, 0, 0, getgenv().res, 0, 0, 0, 1)
                end
            end
        end}
)
N:AddSlider(
    {Name = "Resolution", Min = 0, Max = 1.2, Precise = 2, Value = dy, Callback = function(S)
            getgenv().res = S
        end}
)
local dA, dB, dC, dD, dE = 0.5, Color3.fromRGB(200, 150, 255), false, false, false
local function dF(dG)
    return Color3.new(dG.r, dG.g, dG.b)
end
local function dH(a4)
    local dG = a4.TeamColor
    local dH = Instance.new("Highlight")
    dH.Name = a4.Name
    dH.FillColor = dB
    dH.DepthMode = "AlwaysOnTop"
    dH.FillTransparency = dA
    dH.OutlineTransparency = dA
    dH.Parent = Storage
    local dI = a4.Character
    if dI then
        dH.Adornee = dI
    end
    a4.CharacterAdded:Connect(
        function(dJ)
            dH.Adornee = dJ
        end
    )
    a4:GetPropertyChangedSignal("Team"):Connect(
        function()
            local dK = a4.TeamColor
            dH.FillColor = dF(dK)
        end
    )
end
local function dL()
    for X, Y in ipairs(game.Players:GetPlayers()) do
        if Y == game.Players.LocalPlayer then
            if dC then
                if not Storage:FindFirstChild(Y.Name) then
                    dH(Y)
                end
            else
                local dM = Storage:FindFirstChild(Y.Name)
                if dM then
                    dM:Destroy()
                end
            end
        else
            if not Storage:FindFirstChild(Y.Name) then
                dH(Y)
            end
        end
    end
end
local function dN(af)
    if af then
        dE = true
        local dO = game:GetService("CoreGui")
        local b = game:GetService("Players")
        Storage = Instance.new("Folder")
        Storage.Parent = dO
        Storage.Name = "Highlight_Storage"
        dL()
        coroutine.wrap(
            function()
                while dE do
                    task.wait(0.15)
                    dL()
                end
            end
        )()
    else
        dE = false
        local Storage = game.CoreGui:FindFirstChild("Highlight_Storage")
        if Storage then
            for X, dP in ipairs(Storage:GetChildren()) do
                dP:Destroy()
            end
            Storage:Destroy()
        end
    end
end
local function dQ(af)
    dC = af
    dL()
end
local function dR(af)
    dD = af
    dL()
end
local function dS(dw)
    dB = dw
    local Storage = game.CoreGui:FindFirstChild("Highlight_Storage")
    if Storage then
        for X, dP in ipairs(Storage:GetChildren()) do
            dP.FillColor = dw
        end
    end
end
L:AddToggle(
    {Name = "chams", Value = false, Callback = function(af)
            dN(af)
        end}
)
L:AddColorpicker(
    {Name = "Color", Value = dB, Callback = function(dw, dx)
            dS(dw)
        end}
)
L:AddToggle({Name = "Self", Value = false, Callback = dQ})
L:AddSlider(
    {Name = "Transparency", Min = 0, Max = 1, Value = dA, Callback = function(S)
            dA = S
            local Storage = game.CoreGui:FindFirstChild("Highlight_Storage")
            if Storage then
                for X, dP in ipairs(Storage:GetChildren()) do
                    dP.FillTransparency = S
                    dP.OutlineTransparency = S
                end
            end
        end}
)
L:AddToggle(
    {
        Name = "Cape",
        Value = false,
        Callback = function(S)
            local Y = game.Players.LocalPlayer
            local Z = Y.Character
            local function dT()
                local aL = Z:WaitForChild("Humanoid")
                local dU =
                    aL.RigType == Enum.HumanoidRigType.R15 and Z:WaitForChild("UpperTorso") or Z:WaitForChild("Torso")
                local dV = Instance.new("Part", Z)
                dV.Name = "Cape"
                dV.Anchored = false
                dV.CanCollide = false
                dV.TopSurface = Enum.SurfaceType.Smooth
                dV.BottomSurface = Enum.SurfaceType.Smooth
                dV.Size = Vector3.new(0.2, 0.2, 0.2)
                dV.Transparency = 0
                dV.BrickColor = BrickColor.new("Really black")
                local dW = Instance.new("BlockMesh", dV)
                dW.Scale = Vector3.new(9, 17.5, 0.08)
                local dX = Instance.new("Motor", dV)
                dX.Part0 = dV
                dX.Part1 = dU
                dX.MaxVelocity = 0.01
                dX.C0 = CFrame.new(0, 2, 0) * CFrame.Angles(0, math.rad(90), 0)
                dX.C1 = CFrame.new(0, 1, 0.45) * CFrame.Angles(0, math.rad(90), 0)
                spawn(
                    function()
                        local dY = false
                        while dV.Parent == Z do
                            task.wait(1 / 60)
                            local b4 = 0.1
                            local dZ = dU.Velocity.Magnitude
                            if dY then
                                b4 = b4 + dU.Velocity.Magnitude / 10 * 0.05 + 0.05
                                dY = false
                            else
                                dY = true
                            end
                            b4 = b4 + math.min(dU.Velocity.Magnitude / 11, 0.5)
                            dX.MaxVelocity = math.min(dU.Velocity.Magnitude / 111, 0.04)
                            dX.DesiredAngle = -b4
                            if dX.CurrentAngle < -0.2 and dX.DesiredAngle > -0.2 then
                                dX.MaxVelocity = 0.04
                            end
                            repeat
                                task.wait()
                            until dX.CurrentAngle == dX.DesiredAngle or
                                math.abs(dU.Velocity.Magnitude - dZ) >= dU.Velocity.Magnitude / 10 + 1
                            if dU.Velocity.Magnitude < 0.1 then
                                task.wait(0.1)
                            end
                        end
                    end
                )
            end
            local function d_()
                local dV = Z:FindFirstChild("Cape")
                if dV then
                    dV:Destroy()
                end
            end
            if S then
                if not Z:FindFirstChild("Cape") then
                    dT()
                end
            else
                d_()
            end
        end
    }
)
local e0 = {
    {Name = "Mouse", Value = "Mouse"},
    {Name = "Bottom Center", Value = "BottomCenter"},
    {Name = "Top Center", Value = "TopCenter"},
    {Name = "Left Center", Value = "LeftCenter"},
    {Name = "Right Center", Value = "RightCenter"}
}
local e1 = 1
L:AddDropdown(
    {
        Name = "Tracer Source",
        Default = e0[e1].Name,
        List = {e0[1].Name, e0[2].Name, e0[3].Name, e0[4].Name, e0[5].Name},
        Callback = function(S)
            for e2, e3 in ipairs(e0) do
                if e3.Name == S then
                    e1 = e2
                    break
                end
            end
        end
    }
)
L:AddToggle(
    {
        Name = "Tracer",
        Value = false,
        Callback = function(af)
            Tracer = af
            local e = game:GetService("RunService")
            local b = game:GetService("Players")
            local e4 = game:GetService("Workspace")
            local e5 = game:GetService("UserInputService")
            local j = e4.CurrentCamera
            local settings = {
                DefaultSettings = false,
                TeamCheck = false,
                TracersVisible = true,
                TracerColor = Color3.fromRGB(128, 0, 128),
                TracerThickness = 1.8,
                TracerTransparency = 0.8
            }
            local e6 = {}
            local function e7(Z)
                local cT = Drawing.new("Line")
                cT.Thickness = settings.TracerThickness
                cT.Transparency = settings.TracerTransparency
                cT.Visible = false
                cT.Color = settings.TracerColor
                local function e8()
                    if not Z:IsDescendantOf(e4) or not Z:FindFirstChild("HumanoidRootPart") then
                        cT.Visible = false
                        return
                    end
                    local e9 = Z:FindFirstChild("Head") or Z:FindFirstChild("HumanoidRootPart")
                    if not e9 then
                        cT.Visible = false
                        return
                    end
                    local ea = e9.Position
                    local eb, ec = j:WorldToViewportPoint(ea)
                    local ed = e0[e1]
                    if ed.Value == "Mouse" then
                        cT.From = Vector2.new(e5:GetMouseLocation().X, e5:GetMouseLocation().Y)
                    elseif ed.Value == "BottomCenter" then
                        cT.From = Vector2.new(j.ViewportSize.X / 2, j.ViewportSize.Y)
                    elseif ed.Value == "TopCenter" then
                        cT.From = Vector2.new(j.ViewportSize.X / 2, 0)
                    elseif ed.Value == "LeftCenter" then
                        cT.From = Vector2.new(0, j.ViewportSize.Y / 2)
                    elseif ed.Value == "RightCenter" then
                        cT.From = Vector2.new(j.ViewportSize.X, j.ViewportSize.Y / 2)
                    end
                    if ec then
                        cT.To = Vector2.new(eb.X, eb.Y)
                        if settings.TeamCheck then
                            cT.Visible = b.LocalPlayer.Team ~= Z.Team
                        else
                            cT.Visible = Tracer
                        end
                    else
                        cT.Visible = false
                    end
                end
                e6[Z] = cT
                return cT, e8
            end
            local function ee(Y, Z)
                if Y == b.LocalPlayer then
                    return
                end
                if not Z or not Z:IsDescendantOf(e4) or not Z:FindFirstChild("HumanoidRootPart") then
                    return
                end
                local ef, eg = e7(Z)
                e.RenderStepped:Connect(
                    function()
                        eg()
                    end
                )
                Z.AncestryChanged:Connect(
                    function()
                        if not Z:IsDescendantOf(e4) then
                            ef.Visible = false
                            e6[Z] = nil
                        end
                    end
                )
            end
            b.PlayerAdded:Connect(
                function(Y)
                    Y.CharacterAdded:Connect(
                        function(Z)
                            ee(Y, Z)
                        end
                    )
                end
            )
            for X, Y in ipairs(b:GetPlayers()) do
                ee(Y, Y.Character)
            end
            e5.InputBegan:Connect(
                function(c8)
                    if c8.KeyCode == settings.ModeSkipKey and not e5:GetFocusedTextBox() then
                        e1 = e1 % #e0 + 1
                        for X, cT in pairs(e6) do
                            cT.Visible = settings.TracersVisible and Tracer
                        end
                    elseif c8.KeyCode == settings.DisableKey and not e5:GetFocusedTextBox() then
                        settings.TracersVisible = not settings.TracersVisible
                        for X, cT in pairs(e6) do
                            cT.Visible = settings.TracersVisible and Tracer
                        end
                    end
                end
            )
            if settings.DefaultSettings then
                settings.TeamCheck = false
                settings.TracersVisible = true
                settings.TracerColor = Color3.fromRGB(128, 0, 128)
                settings.TracerThickness = 1.8
                settings.TracerTransparency = 0.8
            end
        end
    }
)
local Y, eh = game.Players.LocalPlayer, false
local function ei(Z)
    local e9 = Z:FindFirstChild("Head")
    if e9 then
        local ej = e9:FindFirstChild("face")
        if ej then
            ej:Destroy()
        end
    end
end
local function ek(ay)
    if ay:IsA("Accessory") or ay:IsA("Shirt") or ay:IsA("Pants") or ay.Name == "Face" then
        ay:Destroy()
    elseif ay:IsA("BasePart") then
        ay.Material = Enum.Material.ForceField
        ay.Color = Color3.fromRGB(225, 160, 255)
    end
end
local function el(Z)
    ei(Z)
    for X, ay in ipairs(Z:GetChildren()) do
        ek(ay)
    end
    Z.ChildAdded:Connect(ek)
end
local function em(Z)
    task.wait(0.14)
    if eh then
        el(Z)
    end
end
L:AddToggle(
    {Name = "Self visual", Value = false, Callback = function(dg)
            eh = dg
            if dg then
                em(Y.Character or Y.CharacterAdded:Wait())
            end
        end}
)
Y.CharacterAdded:Connect(em)
