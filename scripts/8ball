local bF = false
local bG = 20
local bH = 20
local bI, bJ, bK
local function bL()
    local ak, bM = math.huge, nil
    local Y = game.Players.LocalPlayer
    local Z = Y.Character or Y.CharacterAdded:Wait()
    for X, al in pairs(game:GetService("Players"):GetPlayers()) do
        local bN = al.Character
        local bO = bN and bN:FindFirstChild("HumanoidRootPart")
        if al ~= Y and bO and bN.Humanoid.Health > 0 then
            local _ = (Z.HumanoidRootPart.Position - bO.Position).magnitude
            if _ < ak and _ <= bH then
                ak, bM = _, al
            end
        end
    end
    return bM
end
local function bg()
    local Y = game.Players.LocalPlayer
    local Z = Y.Character
    local bc = Z and Z:FindFirstChild("HumanoidRootPart")
    if bc and bI and bI.Character then
        local bP = bI.Character:FindFirstChild("HumanoidRootPart")
        if bP and bI.Character.Humanoid.Health > 0 then
            local t = bP.Position
            if (bc.Position - t).magnitude <= bH then
                task.wait(0.0195)
                if bI ~= bJ then
                    if Y:GetAttribute("Blocking") == true then
                        Y:SetAttribute("Blocking", false)
                        task.wait(0.068)
                        bc.CFrame = CFrame.new(bc.Position):lerp(CFrame.new(t), bG * 0.01)
                        Y:SetAttribute("Blocking", true)
                    else
                        bc.CFrame = CFrame.new(bc.Position):lerp(CFrame.new(t), bG * 0.01)
                    end
                    bJ = bI
                else
                    bc.CFrame = CFrame.new(bc.Position):lerp(CFrame.new(t), bG * 0.01)
                end
