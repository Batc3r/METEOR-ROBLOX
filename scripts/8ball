                local ds = Y:FindFirstChild("PlayerGui")
                if ds then
                    local dt = ds:FindFirstChild("GameSenseWatermark")
                    if dt then
                        local cQ = dt:FindFirstChildOfClass("Frame")
                        if cQ then
                            local bd = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
                            local dz = d:Create(cQ, bd, {Position = UDim2.new(0, -280, 0, 10)})
                            dz:Play()
                            dz.Completed:Connect(
                                function()
                                    dt:Destroy()
                                end
                            )
                        else
                            dt:Destroy()
                        end
                    end
                end
            end
            if S then
                for X, Y in ipairs(b:GetPlayers()) do
                    dr(Y)
                end
                b.PlayerAdded:Connect(dr)
            else
                for X, Y in ipairs(b:GetPlayers()) do
                    dE(Y)
                end
                b.PlayerAdded:Connect(dE)
            end
        end
    }
)
local function dF()
    local dG = game:GetService("Workspace").Camera:FindFirstChild("Viewmodel")
    if dG then
        local dH = dG:FindFirstChild("WoodenSword")
        if dH then
            local dI = dH:FindFirstChild("MainPart")
            if dI then
                return dI:FindFirstChild("Mesh")
            end
        end
    end
    return nil
end
