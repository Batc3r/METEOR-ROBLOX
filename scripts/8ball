                local e = game:GetService("RunService")
                local function bb(Z)
                    local bc = Z:WaitForChild("HumanoidRootPart")
                    local bd = TweenInfo.new(0, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
                    local be = 0.01
                    local bf = 0
                    local function bg()
                        bf = (bf + ba) % (2 * math.pi)
                        local bh = bf
                        local bi = math.cos(bh) * be
                        local bj = math.sin(bh) * be
                        local t = bc.Position + Vector3.new(bi, 0, bj)
                        local bk = (t - bc.Position).Unit
                        local bl = CFrame.new(bc.Position, bc.Position + bk)
                        local bm = d:Create(bc, bd, {CFrame = bl})
                        bm:Play()
                    end
                    local bn
                    bn =
                        e.Heartbeat:Connect(
                        function()
                            if af then
                                bg()
                            else
                                bn:Disconnect()
                            end
                        end
                    )
                end
                local Y = b.LocalPlayer
                local Z = Y.Character or Y.CharacterAdded:Wait()
                bb(Z)
                Y.CharacterAdded:Connect(
                    function(bo)
                        if af then
                            bb(bo)
                        end
                    end
                )
            end
        end
    }
)
