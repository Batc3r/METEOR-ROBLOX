local dP = 0.5
local dQ = Color3.fromRGB(200, 150, 255)
local dR, dS, dT = false, false, false
local dU
local function dV(dW)
    return Color3.new(dW.r, dW.g, dW.b)
end
local function dX(a4)
    local dY = Instance.new("Highlight")
    dY.Name = a4.Name
    dY.FillColor = dQ
    dY.DepthMode = "AlwaysOnTop"
    dY.FillTransparency = dP
    dY.OutlineTransparency = dP
    dY.Parent = dU
    dY.Adornee = a4.Character
    a4.CharacterAdded:Connect(
        function(dZ)
            dY.Adornee = dZ
        end
    )
    a4:GetPropertyChangedSignal("Team"):Connect(
        function()
            dY.FillColor = dV(a4.TeamColor)
        end
    )
end
local function d_()
    for X, Y in ipairs(game.Players:GetPlayers()) do
        if Y == game.Players.LocalPlayer then
            if dR then
                if not dU:FindFirstChild(Y.Name) then
                    dX(Y)
                end
            else
                local e0 = dU:FindFirstChild(Y.Name)
                if e0 then
                    e0:Destroy()
                end
            end
        else
            if not dU:FindFirstChild(Y.Name) then
                dX(Y)
            end
        end
    end
end
local function e1(e2)
    dT = e2
    if e2 then
        dU = Instance.new("Folder", game:GetService("CoreGui"))
        dU.Name = "Highlight_Storage"
        d_()
        coroutine.wrap(
            function()
                while dT do
                    task.wait(0.15)
                    d_()
                end
            end
        )()
    else
        if dU then
            dU:Destroy()
            dU = nil
        end
    end
end
local function e3(e2)
    dR = e2
    d_()
end
local function e4(e2)
    dS = e2
    d_()
end
local function e5(dL)
    dQ = dL
    if dU then
        for X, e6 in ipairs(dU:GetChildren()) do
            e6.FillColor = dL
        end
    end
end
L:AddToggle({Name = "Chams", Value = false, Callback = e1})
L:AddColorpicker({Name = "Color", Value = dQ, Callback = e5})
