local af = false
local aF = 10
local aG = 20
local aH = 0.3
local aI = 0.001
local ag
local aJ = false
local function aK(aL)
    aI = aI + aL
    if aI < aH then
        return
    end
    aI = 0
    local b = game:GetService("Players")
    local Y = b.LocalPlayer
    local Z = Y.Character
    if not Z then
        return
    end
    local aM = Z:FindFirstChild("Humanoid")
    if not aM then
        return
    end
    local aN = Z:FindFirstChild("HumanoidRootPart")
    if not aN then
        return
    end
    local aO = aN.CFrame
    local _ = (aO.Position - lastValidPosition.Position).Magnitude
    if _ > aF and _ < aG then
        local aC = aM.MoveDirection * aM.WalkSpeed
        local aP = lastValidPosition.Position + aC * aH
        if (aO.Position - aP).Magnitude > aF then
            Z:SetPrimaryPartCFrame(lastValidPosition)
        else
            lastValidPosition = aO
        end
    else
        lastValidPosition = aO
    end
    if aJ then
        local aQ = aN.Position
        local aR = lastValidPosition.Position
        local aS = (aQ - aR).Magnitude
        if aS > aG then
            local aT = (aQ - aR).unit
            aN.CFrame = CFrame.new(aR + aT * aG)
        end
    end
end
