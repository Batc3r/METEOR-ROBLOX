    local bB = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if bB then
        bB.Anchored = true
    end
    while tick() - bA <= bt do
        if bB then
            bB.Velocity = bz(by(bB.Velocity.X, -bv, bv), by(bB.Velocity.Y, -bw, bw), by(bB.Velocity.Z, -bx, bx))
        end
        task.wait()
    end
    if bB then
        bB.Anchored = false
    end
end
